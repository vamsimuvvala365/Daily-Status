let feedback = "Out Standing"
if (feedback === "Out Standing") {
    console.log("you are eligible ")
}
let rating = 5
if (rating >= 3) {
    console.log("You Passed")
} else if (rating === 2) {
    console.log("you are fairly eligible for this")
}
else if (rating === 1) {
    console.log("You are doubt to pass")
}
else {
    console.log("you are not eligible")
}
